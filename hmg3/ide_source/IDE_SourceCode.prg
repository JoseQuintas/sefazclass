		if i == 0
			Return
		endif

		if MsgYesNo ( aMsgs [ 168 ]  , aMsgs [ 167 ] + ' ' + ALLTRIM( Project.List_4.Item (i) ) ) == .T.

#ifdef _ALLOW_DELETE_FILE_
			delete file ( ALLTRIM( Project.List_4.Item (i) ) )
