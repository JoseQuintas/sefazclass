function iscontrolinsplit(n)
local i

	for i := 1 to spb.list_1.itemcount
		if ALLTRIM(HMG_UPPER( spb.list_1.Cell ( i , 2 ) ) ) != 'WINDOW'
			if ALLTRIM(HMG_UPPER(n)) == ALLTRIM(HMG_UPPER( spb.list_1.Cell ( i , 1 ) ) )
				msgstop( aMsgs [ 190 ] ,'HMG IDE')
				return .t.
			endif
		endif
	next i

return .f.

