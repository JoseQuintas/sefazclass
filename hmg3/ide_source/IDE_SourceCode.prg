	ContextMenuBuilder( 'D' , HMG_LEN ( _hmgide_aDropDownMenus ) , cNewName )

	if _HMG_SYSDATA [ 257 ] == .T.

		DropDownMenuBuilder.List1.DeleteItem ( HMG_LEN (_hmgide_aDropDownMenus) )

