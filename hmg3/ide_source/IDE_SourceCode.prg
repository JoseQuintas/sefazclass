Local c
Local aTemp := {}
Local x

	i := TbDesigner.List1.Value

	if i == 0
		Return
	EndIf

	c	:= TbDesigner.List1.ItemCount

	for x := 1 to c

		if x == i
			aadd ( aTemp , '' )
		endif

		aadd ( aTemp , TbDesigner.List1.Item (x) )

	next x

	TbDesigner.List1.DeleteAllItems

	for x := 1 to HMG_LEN ( aTemp)
		TbDesigner.List1.AddItem ( aTemp [x] )
	next x

	TbDesigner.List1.Value := i

	aadd ( _TbNames		, '' )
	aadd ( _TbCaptions	, '' )
