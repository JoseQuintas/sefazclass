				EndIf

				* Get Width

				x := HB_UTF8RAT ( ' WIDTH ' , HMG_UPPER(Line) )

				if x > 0 .and. !IsLiteral ( Line , x )
					cWidth := HB_USUBSTR ( Line , x + 7 , 255 )
					Line := HB_ULEFT ( Line , x )
				Else
					cWidth := ''
				EndIf

				aadd ( aList , '$KEYBOARD' )

				aadd ( 	_StCaptions , '$KEYBOARD' )
				aadd ( 	_StActions , Action )
