				+ if ( !Empty(_StToolTips[i])	, ' TOOLTIP '	+ chr(34) + _StToolTips[i] + chr(34) , '' ) ;
				+ CRLF

		EndIf

	Next i

	Menu := Menu + Space(4) + 'END STATUSBAR' + CRLF

	_hmgide_CurrentStatusBar := Menu

	DesignFormModified := .T.

	StDesigner.Release


Return
