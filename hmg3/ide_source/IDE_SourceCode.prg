*------------------------------------------------------------------------------*
Procedure StNext
*------------------------------------------------------------------------------*
Local i, c

	i := StDesigner.List1.Value

	if i == 0
		Return
	EndIf

	c	:= StDesigner.List1.ItemCount

	if i + 1 > c

		if !Empty ( StDesigner.List1.Item (i) )

			StDesigner.List1.additem( '' )

			aadd ( _StCaptions	, '' )
			aadd ( _StActions	, '' )
			aadd ( _StWidths	, '' )
			aadd ( _StIcons		, '' )
			aadd ( _StToolTips	, '' )
			aadd ( _StStyles	, 1  )

