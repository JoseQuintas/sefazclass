		endif

		if MsgYesNo ( aMsgs [ 168 ]  , aMsgs [ 167 ] + ' ' + ALLTRIM( Project.tables.Item (i) ) ) == .T.

#ifdef _ALLOW_DELETE_FILE_
			delete file ( ALLTRIM( Project.tables.Item (i) ) )
#endif

			Project.tables.DeleteItem (i)

		endif



	EndIf

