	StDesigner.List1.Value := i + 1

	StDesigner.Text1.Value := StDesigner.List1.Item(i+1)

	setfocus ( GetControlHandle ( 'Text1' , 'StDesigner' ) )

	if HMG_LEN ( _StActions ) >= i+1

