Function IsControNameValid ( cNewName )
*------------------------------------------------------------------------------*
Local i
Local l
Local RetVal := .T.
local f := .f.
local c


	for i := 1 To HMG_LEN ( cNewName )

		c := HMG_UPPER ( HB_USUBSTR ( cNewName , i , 1 ) )

		if	c <> 'A'	;
			.AND.		;
			c <> 'B'	;
			.AND.		;
			c <> 'C'	;
			.AND.		;
			c <> 'D'	;
			.AND.		;
			c <> 'E'	;
			.AND.		;
			c <> 'F'	;
