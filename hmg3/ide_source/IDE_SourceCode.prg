				+ if ( !Empty(_StToolTips[i])	, ' TOOLTIP '	+ chr(34) + _StToolTips[i] + chr(34) , '' ) ;
				+ CRLF

		Else

			* Item

			Menu := Menu + Space(8) + 'STATUSITEM ' + chr(34) + _StCaptions [ i ] + chr(34) ;
				+ if ( !Empty(_StWidths[i])	, ' WIDTH '	+ _StWidths[i]	, '' ) ;
				+ if ( !Empty(_StActions[i])	, ' ACTION '	+ _StActions[i]	, '' ) ;
