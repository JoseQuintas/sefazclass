*------------------------------------------------------------------------------*
Procedure StMoveDown
*------------------------------------------------------------------------------*
Local i
Local c
Local Next
Local Current
Local aux0
Local aux1
Local aux2
Local aux3
Local aux4
Local aux5

	i := StDesigner.List1.Value

	if i == 0
		Return
	EndIf

	c := StDesigner.List1.ItemCount

