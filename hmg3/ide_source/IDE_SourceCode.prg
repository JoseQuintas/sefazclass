Function HitTest ( mr , mc , cr , cc , cw , ch )
Local RetVal := .F.

	If 	mr >= cr ;
		.and. ;
	 	mr <= ( cr + ch ) ;
		.and. ;
	 	mc >= cc  ;
		.and. ;
	 	mc <= ( cc + cw )

		RetVal := .T.

	EndIf

Return RetVal

