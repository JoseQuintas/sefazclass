Function RemoveExtraSpaces (Line)
*------------------------------------------------------------------------------*
Local i , l , c , p , NewLine

	l := HMG_LEN (Line)

	p := ''
	NewLine := ''

	For i := 1 To l

		c := HB_USUBSTR ( Line , i , 1 )

		if ! ( c == ' ' .and. p == ' ' )

			if c != chr (8)
				NewLine := NewLine + c
			endif

		EndIf

		p := c

	Next i

Return ( NewLine )

