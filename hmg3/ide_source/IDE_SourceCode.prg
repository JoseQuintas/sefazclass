*------------------------------------------------------------------------------*
// Procedure PointCount ( String )
Function PointCount ( String )
*------------------------------------------------------------------------------*
Local i
Local PointCount := 0

	For i := 1 To HMG_LEN ( String )
		If HB_USUBSTR ( String , i , 1 ) == '.'
			PointCount++
		Else
			Exit
		EndIf
	Next i

Return PointCount

