	TbDesigner.Text3.Value := ''
	TbDesigner.Text4.Value := ''
	TbDesigner.Text4a.Value := ''

	TbDesigner.Check5.Value := .f.
	TbDesigner.Check6.Value := .f.
	TbDesigner.Check7.Value := .f.
	TbDesigner.Check8.Value := .f.

