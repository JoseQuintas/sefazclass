*------------------------------------------------------------------------------*
// Procedure PointRemove ( String )
Function PointRemove ( String )
*------------------------------------------------------------------------------*
Local i
Local PointCount := 0

	String := ALLTRIM ( String )

	For i := 1 To HMG_LEN ( String )
		If HB_USUBSTR ( String , i , 1 ) == '.'
			PointCount++
		Else
			Exit
		EndIf
	Next i

	String := ALLTRIM ( HB_URIGHT ( String , HMG_LEN(String) - PointCount ) )

Return String

