	_TbPictures	[ i ] := ''
	_TbActions	[ i ] := ''
	_TbToolTips	[ i ] := ''

	_TbSeparators [i] := .f.
	_TbAutoSizes [i] := .f.
	_TbChecks 	[i] := .f.
